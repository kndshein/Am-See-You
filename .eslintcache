ted\n    return arrayOfPromises;\n  };\n\n  React.useEffect(() => {\n    //Pass the array of promises to Promise.all to make sure they have all resolved\n    Promise.all(getAllMovies())\n      // The returned values from promise.all can then be handled in the callback via .then that'll run when Promise.all complete\n      .then((arrayOfData) => setMoviesListState(arrayOfData));\n  }, []);\n\n  return (\n    <div className=\"movies-list\">\n      <SingleMovie\n        moviesListState={moviesListState}\n        handleClickAddWatched={props.handleClickAddWatched}\n      />\n    </div>\n  );\n};\nexport default MoviesList;\n","/home/kndshein/Documents/General Assembly/Unit 2/projecttwo/src/components/Nav.js",["44"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\n\nconst Nav = (props) => {\n  return (\n    <div className=\"nav-bar\">\n      {/* <Link to=\"/\" className=\"nav-list\">\n        Movies List\n      </Link> */}\n      <div className=\"img-container\">\n        <img\n          src=\"https://media.giphy.com/media/XmppNRlrlu2SA/giphy.gif\"\n          alt=\"futuristic blob looking thang\"\n        />\n      </div>\n      {/* <Link to=\"/watched\" className=\"nav-list\">\n        Watched List\n      </Link> */}\n    </div>\n  );\n};\n\nexport default Nav;\n","/home/kndshein/Documents/General Assembly/Unit 2/projecttwo/src/components/SingleMovie.js",[],"/home/kndshein/Documents/General Assembly/Unit 2/projecttwo/src/components/GenresList.js",[],{"ruleId":"45","replacedBy":"46"},{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","severity":1,"message":"50","line":28,"column":6,"nodeType":"51","endLine":28,"endColumn":8,"suggestions":"52"},{"ruleId":"53","severity":1,"message":"54","line":2,"column":10,"nodeType":"55","messageId":"56","endLine":2,"endColumn":14},"no-native-reassign",["57"],"no-negated-in-lhs",["58"],"react-hooks/exhaustive-deps","React Hook React.useEffect has a missing dependency: 'getAllMovies'. Either include it or remove t